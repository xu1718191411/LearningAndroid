<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.syoui.imagetab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".detailActivity"
            android:label="@string/title_activity_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".listViewActivity" />
        <activity
            android:name=".subListViewActivity"
            android:label="@string/title_activity_sub_list_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".contentActivity" />
        <activity android:name=".videoActivity" />
        <activity android:name=".foundation.activity.foundation_activity_listView" />
        <activity android:name=".foundation.activity.foundation_activity_foldingListView" />
        <activity android:name=".foundation.activity.foundation_activity_launcher" />
        <activity android:name=".foundation.activity.foundation_activity_setting"></activity>
    </application>

</manifest>